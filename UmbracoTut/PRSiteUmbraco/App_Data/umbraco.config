      <blogPost id="1149" key="a3b3bb6f-4752-4c05-925b-bc04715eb4a6" parentID="1108" level="3" creatorID="0" sortOrder="4" createDate="2017-04-10T11:36:03" updateDate="2017-04-10T11:36:13" nodeName="IPhone7-Gray" urlName="iphone7-gray" path="-1,1107,1108,1149" isDoc="" nodeType="1129" creatorName="thien.tt" writerName="thien.tt" writerID="0" template="1128" nodeTypeAlias="blogPost">
