      <blogPost id="1150" key="771b88e2-74c9-43fe-af29-24bb5fd3ac6c" parentID="1108" level="3" creatorID="0" sortOrder="5" createDate="2017-04-10T11:36:33" updateDate="2017-04-10T11:36:39" nodeName="IPhone8" urlName="iphone8" path="-1,1107,1108,1150" isDoc="" nodeType="1129" creatorName="thien.tt" writerName="thien.tt" writerID="0" template="1128" nodeTypeAlias="blogPost">
        <contentGrid><![CDATA[{
  "name": "1 column layout",
  "sections": [
    {
      "grid": 12,
