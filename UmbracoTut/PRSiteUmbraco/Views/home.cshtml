    @*Render featured section*@
    @*@{ Html.RenderAction("RenderFeaturedSection", "Home");}*@
    @{ Html.RenderPartial(string.Format("{0}{1}.cshtml", PARTIAL_VIEW_FOLDER_HOME, "_Featured"));}

    @*Render service section*@
    @*@{ Html.RenderAction("RenderServicesSection", "Home");}*@
    @{ Html.RenderPartial(string.Format("{0}{1}.cshtml", PARTIAL_VIEW_FOLDER_HOME, "_Services"));}
